};
const bookColors = {"New York": "#1976d2", "New York Workbook": "#1976d2", "London": "#b71c1c", "London Workbook": "#b71c1c", "Sydney": "#43a047", "Sydney Workbook": "#43a047"};

const mainView = document.getElementById("mainView");
const audioPlayer = document.getElementById("audioPlayer");
const audioSource = document.getElementById("audioSource");

function clearMain() {
  mainView.innerHTML = '';
}

function playAudio(url) {
  audioSource.src = url;
  audioPlayer.load();
  audioPlayer.play();
}

function renderBooks() {
  clearMain();
  for (let book in data) {
    const btn = document.createElement("button");
    btn.textContent = book;
    const baseColor = bookColors[book] || "#607d8b";
    btn.style.backgroundColor = baseColor + "cc";
    btn.onclick = () => renderLessons(book);
    mainView.appendChild(btn);
  }
}

function renderLessons(book) {
  clearMain();
  const back = document.createElement("button");
  back.textContent = book;
  back.style.backgroundColor = bookColors[book] || "#607d8b";
  back.onclick = renderBooks;
  mainView.appendChild(back);

  const lessons = data[book];
  for (let lesson in lessons) {
    const btn = document.createElement("button");
    btn.textContent = lesson;
    const baseColor = bookColors[book] || "#607d8b";
    btn.style.backgroundColor = baseColor + "cc";
    btn.onclick = () => renderActivities(book, lesson);
    mainView.appendChild(btn);
  }
}

function renderActivities(book, lesson) {
  clearMain();
  const back = document.createElement("button");
  back.textContent = book + " / " + lesson;
  back.style.backgroundColor = bookColors[book] || "#607d8b";
  back.onclick = () => renderLessons(book);
  mainView.appendChild(back);

  const activities = data[book][lesson];
  for (let item of activities) {
    const btn = document.createElement("button");
    btn.textContent = item.title;
    const baseColor = bookColors[book] || "#607d8b";
    btn.style.backgroundColor = baseColor + "cc";
    btn.onclick = () => playAudio(item.url);
    mainView.appendChild(btn);
  }
}

renderBooks();
</script>

</body>
</html>
